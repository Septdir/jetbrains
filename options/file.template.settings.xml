<application>
  <component name="ExportableFileTemplateSettings">
    <includes_templates>
      <template name="[Joomla] Languages - File.ini" file-name="joomla.languages.file" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Header.xml" file-name="joomla.manifest.header.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Information.xml" file-name="joomla.manifest.information.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Languages.xml" file-name="joomla.manifest.languages.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Layouts.xml" file-name="joomla.manifest.layouts.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Media.xml" file-name="joomla.manifest.media.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Updates.xml" file-name="joomla.manifest.updates.xml" reformat="true" live-template-enabled="false" />
    </includes_templates>
    <default_templates>
      <template name="[Joomla] Component - Access File.xml" file-name="access.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Component - Config File.xml" file-name="config.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Languages.ini" file-name="language/en-GB/${NAME}" reformat="true" live-template-enabled="false">
        <template name="[Joomla] Languages.ini.child.2.ini" file-name="language/en-GB/${NAME}.sys" reformat="true" live-template-enabled="false" />
        <template name="[Joomla] Languages.ini.child.1.ini" file-name="language/ru-RU/${NAME}.sys" reformat="true" live-template-enabled="false" />
        <template name="[Joomla] Languages.ini.child.0.ini" file-name="language/ru-RU/${NAME}" reformat="true" live-template-enabled="false" />
      </template>
      <template name="[Joomla] Languages.ini.child.0.ini" file-name="language/ru-RU/${NAME}" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Languages.ini.child.1.ini" file-name="language/ru-RU/${NAME}.sys" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Languages.ini.child.2.ini" file-name="language/en-GB/${NAME}.sys" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Component.xml" file-name="${name}.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Module.xml" file-name="mod_${name}.xml" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Package.xml" file-name="pkg_${name}" reformat="true" live-template-enabled="false" />
      <template name="[Joomla] Manifests - Plugin.xml" file-name="${name}.xml" reformat="true" live-template-enabled="false" />
    </default_templates>
  </component>
</application>